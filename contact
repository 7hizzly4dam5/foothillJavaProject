/** 
 * A representation of a contact with fields for each piece of information and getters/setters for each field.
 * 
 */
/** 
 * EP made changes to 
 *
 */
public class Contact
{
	String firstName;
	String lastName;
	String street;
	String city;
	String zip;
	String state;
	String email;
	String phone;
	String notes;
	
	public String getFirstName()
	{
		return firstName;
	}
	
	public void setFirstName(String newFirstName)
	{
		firstName = newFirst;
	}
	
	public String getLastName()
	{
		return lastName;
	}
	
	public void setLast(String newLastName)
	{
		lastName = newLastName;
	}
	
	public String getStreet()
	{
		return street;
	}
	
	public void setStreet(String newStreet)
	{
		street = newStreet;
	}
	
	public String getCity()
	{
		return city;
	}
	
	public void setCity(String newCity)
	{
		city = newCity;
	}
	
	public String getZip()
	{
		return zip;
	}
	
	public void setZip(String newZip)
	{
		zip = newZip;
	}
	
	public String getState()
	{
		return state;
	}
	
	public void setState(String newState)
	{
		state = newState;
	}
	
	public String getEmail()
	{
		return email;
	}
	
	public void setEmail(String newEmail)   //added by EP
	{
		email = newEmail;
	}
	
	public String getPhone()
	{
		return phone;
	}
	
	public void setPhone(String newPhone)
	{
		phone = newPhone;
	}
	
	public String getNotes()
	{
		return notes;
	}
	
	public void setNotes(String newNotes)
	{
		notes = newNotes;
	}
	
	 private String first;
   private String last;
   private String street;
   private String city;
   private String zip;
   private String state;
   private String state;
   private String email;
   private String phone;
   private String notes;
	
	 public void read()
	 {
	    /** 
	    * This method prompts the user to input information for the calling Contact object
	    * , which is then written to each data member of that contact
	    * Parameters: none
	    * Action: Prompts the user to input information for each of the Contact
	    *               data members
	    */
	 }
	   
	 
	public String getFirst()
	{
		/**
		 * Returns the first data member of the calling contact
		 */
	}

	public String getLast()
	{
		/**
		 * Returns the last data member of the calling contact
		 */
	}
	
	public String getStreet()
	{
		/**
		 * Returns the street data member of the calling contact
		 */
	}
	
	public String getCity()
	{
		/**
		 * Returns the city data member of the calling contact
		 */
	}
	
	public String getZip()
	{
		/**
		 * Returns the zip data member of the calling contact
		 */
	}
	
	public String getState()
	{
		/**
		 * Returns the state data member of the calling contact
		 */
	}
	
	public String getEmail()
	{
		/**
		 * Returns the email data member of the calling contact
		 */
	}
	
	public String getPhone()
	{
		/**
		 * Returns the phone data member of the calling contact
		 */
	}
	
	public String getNotes()
	{
		/**
		 * Returns the notes data member of the calling contact
		 */
	}
}

